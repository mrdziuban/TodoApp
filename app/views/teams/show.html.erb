<h1><%= @team.name %></h1>

<h3>Projects:</h3>
<ul>
  <% @team.projects.each do |proj| %>
    <%= link_to proj.title, project_url(proj) %>
  <% end %>
</ul>

<h3>Members:</h3>
<ul>
  <% @team.users.each do |user| %>
    <%= link_to user.name, user_url(user) %>
  <% end %>
</ul>

<br>
<%= link_to "New project", new_team_project_url(@team.id) %>
<br>
<%= link_to "Edit team", edit_team_url(@team) %>
<%= button_to "Delete team", team_url(@team), method: :delete %>